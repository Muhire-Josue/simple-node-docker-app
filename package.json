{
  "name": "express-app-starter",
  "version": "1.0.0",
  "description": "This is a quicker starter template for express and node applications",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js --exec babel-node --presets @babel/preset-env",
    "start": "babel-node index.js",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrations": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "test": "cross-env NODE_ENV=test && nyc mocha --require @babel/register --timeout 10000 --exit ./server/tests/**/*.js",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Muhire-Josue/express-app-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Muhire-Josue/express-app-starter/issues"
  },
  "homepage": "https://github.com/Muhire-Josue/express-app-starter#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "babel-eslint": "^10.1.0",
    "babel-node": "0.0.1-security",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "method-override": "^2.3.10",
    "morgan": "^1.10.0",
    "node-cron": "^2.0.3",
    "pg": "^7.18.2",
    "regenerator-runtime": "^0.13.5",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-test-helpers": "^1.2.2",
    "uuid": "^7.0.3",
    "zombie": "^6.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "underscore": "^1.10.2"
  }
}
